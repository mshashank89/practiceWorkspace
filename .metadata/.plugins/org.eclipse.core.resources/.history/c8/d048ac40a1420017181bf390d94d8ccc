package ds.arrays;

public class ArrayOperations {
	
	static int[] a;
	static{
		a = new int[5];
		for (int i=0;i<a.length;i++){
			a[i] = i+1;
		}
	}
	public static void main(String[] args) {
		
		printReverse();

	}
	
	public static  void printReverse(){
		
		for (int i=a.length-1;i>=0;i--){
			System.out.println(a[i]);
		}
		
	}

}
