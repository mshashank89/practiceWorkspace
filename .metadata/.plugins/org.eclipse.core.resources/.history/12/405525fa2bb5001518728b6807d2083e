	public static LoyPromoBeanTreeNode buildTreeFromList(
			List<LoyPromoBean> promoBeanList) {

		Map<String, LoyPromoBean> beanMap = new HashMap<String, LoyPromoBean>();
		LoyPromoBeanTreeNode promoTreeRootNode = new LoyPromoBeanTreeNode();

		for (LoyPromoBean promoBean : promoBeanList) {
			beanMap.put(promoBean.getLoyPromoRLId(), promoBean);
			if ("".equals(promoBean.getParRowId())) {
				promoTreeRootNode = promoBean;
			}
		}

		for (LoyPromoBean promoBean : promoBeanList) {

		}

		return null;
	}
	
	

