package hackerrank.bigdecimal;

import java.math.BigDecimal;
import java.util.*;


/*

Sample Input
----------------
9
-100
50
0
56.6
90
0.12
.12
02.34
000.000



4
31
2
4
3


Sample Output
----------------
90
56.6
50
02.34
0.12
.12
0
000.000
-100

 */
class Solution{

    public static void main(String []args){
        //Input
        Scanner sc= new Scanner(System.in);
        int n=sc.nextInt();
        String []s=new String[n+2];
        for(int i=0;i<n;i++){
            s[i]=sc.next();
        }
      	sc.close();
      	
      	for (int i=0;i<n;i++){
      		for (int j=i;j<n-1;j++){
//      			BigDecimal b1 = BigDecimal.valueOf(Double.valueOf(s[i]));
//      			BigDecimal b2 = BigDecimal.valueOf(Double.valueOf(s[j]));
      			
      			Double d1 = Double.valueOf(s[j]);
      			Double d2 = Double.valueOf(s[j+1]);
      			
      			if (d1 < d2){      			
      				swap(s, j, j+1);
      			}
      			
      		}
      	}
      	
        //Output
        for(int i=0;i<n;i++)
        {
            System.out.println(s[i]);
        }
    }
    
    public static void swap (String[] s, int i, int j){
    	String temp = s[i];
    	s[i] = s[j];
    	s[j] = temp;
    }

}